[
  {
    "name": "Date Formatting I",
    "commands": [
      {
        "key": "%Y",
        "description": "Year with century (e.g. 2015, 1995, etc)"
      },
      {
        "key": "%m",
        "description": "Month of the year, zero-padded (01..12)"
      },
      {
        "key": "%B",
        "description": "The full month name (e.g. January)"
      },
      {
        "key": "%b",
        "description": "The abbreviated month name (e.g. Jan)"
      },
      {
        "key": "%d",
        "description": "Day of the month, zero-padded (01..31)"
      },
      {
        "key": "%j",
        "description": "Day of the year (001..366)"
      }
    ]
  },
  {
    "name": "Flags I",
    "commands": [
      {
        "key": "-",
        "description": "Don't pad a numerical output"
      },
      {
        "key": "_",
        "description": "Use spaced for padding"
      },
      {
        "key": "0",
        "description": "Use zeros for padding"
      },
      {
        "key": "^",
        "description": "Upcase the result string"
      },
      {
        "key": "#",
        "description": "Change case"
      },
      {
        "key": ":",
        "description": "Use colons for %z"
      }
    ]
  },
  {
    "name": "Date Formatting II",
    "commands": [
      {
        "key": "%C",
        "description": "Year / 100 (round down. e.g. 20 in 2015)"
      },
      {
        "key": "%y",
        "description": "Year % 100 (00...99)"
      },
      {
        "key": "%_m",
        "description": "Month of the year, blank-padded (_1..12)"
      },
      {
        "key": "%-m",
        "description": "Month of the year, no-padding (1..12)"
      },
      {
        "key": "%^B",
        "description": "The full month name uppercased (e.g. JANUARY)"
      },
      {
        "key": "%^b",
        "description": "The abbreviated month name uppercased (e.g. JAN)"
      },
      {
        "key": "%h",
        "description": "Equivalent to %b (abbreviated month name)"
      },
      {
        "key": "%-d",
        "description": "Day of the month, no-padding (1..31)"
      },
      {
        "key": "%e",
        "description": "Day of the month, blank-padded (_1..31)"
      }
    ]
  },
  {
    "name": "Time Formatting I",
    "commands": [
      {
        "key": "%H",
        "description": "Hour of the day, 24-hour clock, zero-padded (00..23)"
      },
      {
        "key": "%k",
        "description": "Hour of the day, 24-hour clock, blank-padded (_0..23)"
      },
      {
        "key": "%I",
        "description": "Hour of the day, 12-hour clock, zero-padded (01..12)"
      },
      {
        "key": "%l",
        "description": "Hour of the day, 12-hour clock, blank-padded (_1..12)"
      },
      {
        "key": "%P",
        "description": "Meridian indicator, lowercase (am or pm)"
      },
      {
        "key": "%p",
        "description": "Meridian indicator, uppercase (AM or PM)"
      },
      {
        "key": "%M",
        "description": "Minute of the hour (00..59)"
      },
      {
        "key": "%S",
        "description": "Second of the minute (00..59)"
      }
    ]
  },
  {
    "name": "Time Zone Formatting I",
    "commands": [
      {
        "key": "%z",
        "description": "Time zone as hour and minute offset from UTC (e.g. +0900)"
      },
      {
        "key": "%:z",
        "description": "Time zone hour and minute offset from UTC with a colon (e.g. +09:00)"
      },
      {
        "key": "%::z",
        "description": "Time zone hour, minute and second offset from UTC (e.g. +09:00:00)"
      },
      {
        "key": "%:::z",
        "description": "Time zone hour, minute and second offset from UTC (e.g. +09, +09:30, +09:30:30)"
      },
      {
        "key": "%Z",
        "description": "Time zone abbreviation name or something similar information"
      }
    ]
  },
  {
    "name": "Weekday Formatting I",
    "commands": [
      {
        "key": "%A",
        "description": "The full weekday name (e.g. Sunday)"
      },
      {
        "key": "%^A",
        "description": "The full weekday name uppercased (e.g. SUNDAY)"
      },
      {
        "key": "%a",
        "description": "The abbreviated weekday name (e.g. Sun)"
      },
      {
        "key": "%^a",
        "description": "The abbreviated weekday name uppercased (e.g. SUN)"
      },
      {
        "key": "%u",
        "description": "Day of the week starting Monday (1..7)"
      },
      {
        "key": "%w",
        "description": "Day of the week starting Sunday (0..6)"
      }
    ]
  },
  {
    "name": "Week Formatting I",
    "commands": [
      {
        "key": "%G",
        "description": "The week-based year"
      },
      {
        "key": "%g",
        "description": "The last 2 digits of the week-based year (00..99)"
      },
      {
        "key": "%V",
        "description": "Week number of the week-based year (01..53)"
      },
      {
        "key": "%U",
        "description": "Week number of the year. Week starts with Sunday (00..53)"
      },
      {
        "key": "%W",
        "description": "Week number of the year. Week starts with Monday (00..53)"
      }
    ]
  },
  {
    "name": "Seconds Formatting I",
    "commands": [
      {
        "key": "%s",
        "description": "Number of seconds since 1970-01-01 00:00:00 UTC"
      },
      {
        "key": "%Q",
        "description": "Number of milliseconds since 1970-01-01 00:00:00 UTC"
      }
    ]
  },
  {
    "name": "Literal Strings I",
    "commands": [
      {
        "key": "%n",
        "description": "Newline character (\\n)"
      },
      {
        "key": "%t",
        "description": "Tab character (\\t)"
      },
      {
        "key": "%%",
        "description": "Literal '%' character"
      }
    ]
  },
  {
    "name": "Combination I",
    "commands": [
      {
        "key": "%c",
        "description": "Date and time (%a %b %e %T %Y)"
      },
      {
        "key": "%D",
        "description": "Date (%m/%d/%y)"
      },
      {
        "key": "%F",
        "description": "The ISO 8601 date format (%Y-%m-%d)"
      },
      {
        "key": "%v",
        "description": "VMS date (%e-%b-%Y)"
      },
      {
        "key": "%x",
        "description": "Same as %D"
      },
      {
        "key": "%X",
        "description": "Same as %T"
      },
      {
        "key": "%r",
        "description": "12-hour time (%I:%M:%S %p)"
      },
      {
        "key": "%R",
        "description": "24-hour time (%H:%M)"
      },
      {
        "key": "%T",
        "description": "24-hour time (%H:%M:%S)"
      },
      {
        "key": "%+",
        "description": "date(1) (%a %b %e %H:%M:%S %Z %Y)"
      }
    ]
  },
  {
    "name": "Time Formatting II",
    "commands": [
      {
        "key": "%L",
        "description": "Millisecond of the second (000..999)"
      },
      {
        "key": "%N",
        "description": "Nanosecond (9 digits)"
      },
      {
        "key": "%3N",
        "description": "Millisecond (3 digits)"
      },
      {
        "key": "%6N",
        "description": "Microsecond (6 digits)"
      },
      {
        "key": "%12N",
        "description": "Picosecond (12 digits)"
      },
      {
        "key": "%15N",
        "description": "Femtosecond (15 digits)"
      },
      {
        "key": "%18N",
        "description": "Attosecond (18 digits)"
      },
      {
        "key": "%21N",
        "description": "Zeptosecond (21 digits)"
      },
      {
        "key": "%24N",
        "description": "Yoctosecond (24 digits)"
      }
    ]
  }
]